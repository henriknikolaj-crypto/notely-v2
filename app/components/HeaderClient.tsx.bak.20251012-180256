"use client";
import Link from "next/link";
import { usePathname } from "next/navigation";

// Client-wrapper, men selve nav'en rendres server-side via <ServerHeader/>.
// Denne fil sikrer aktiv-link markering i client-land (valgfrit).
export default function HeaderClient({ children }: { children: React.ReactNode }) {
  const pathname = usePathname();
  return <div data-pathname={pathname}>{children}</div>;
}
